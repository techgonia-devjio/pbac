{
  "name": "techgonia/pbac-module",
  "type": "library",
  "version": "1.0.0",
  "description": "PBAC module for starter kit.",
  "autoload": {
    "psr-4": {
      "Modules\\Pbac\\": "src/",
      "Modules\\Pbac\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.4",
    "illuminate/database": "^12.34",
    "illuminate/support": "^12.0",
    "illuminated/helper-functions": "^12.0",
    "laravel/tinker": "^2.10.1",
    "spatie/laravel-package-tools": "^1.92"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23",
    "laravel/pail": "^1.2.2",
    "laravel/pint": "^1.13",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^8.6",
    "pestphp/pest": "^3.8",
    "phpunit/phpunit": "^11.5.3"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Modules\\Pbac\\Providers\\PbacServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": false
}
